{% extends 'base.html.twig' %}
{% block title %}{{ 'Notre ADN | Impact-performances' }}{% endblock %}
{% block slide %}
    <div id="tg-innerbanner" class="tg-innerbanner tg-innerbannervfour">
        <figure data-vide-bg="poster: {{ asset('build/global/static/images/innerpagebanner/img-04.jpg') }}" data-vide-options="position: 0% 50%">
            <figcaption>
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <h1>{{ 'Notre ADN' }}</h1>
                            <h2>{{ 'Une toute nouvelle façon de développer votre entreprise'|capitalize }}</h2>
                        </div>
                    </div>
                </div>
                <a class="tg-btnscrolldown tg-btnscrolldownvtwo" href="#tg-main"></a>
            </figcaption>
        </figure>
    </div>
{% endblock %}
{% block body %}
        <section id="tg-main" class="tg-sectionspace tg-haslayout tg-paddingbottomzero">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-push-2 col-lg-8">
                        <div class="tg-sectionhead tg-sectionheadvtwo">
                             <div class="tg-sectiontitle">
                                   <h2>{{'Notre ADN'|upper }}</h2>
                             </div>
                            <div class="tg-description">
                                <p>Mauris in rutrum nunc. Aenean in lectus id mi sollicitudin placerat. Sed porta ac libero eget maximus. Donec dictum, sem quis ullamcorper porta,</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--************************************
            Content Area Start
        *************************************-->
        <section class="tg-haslayout">
            <div class="container-fluid">
                <div class="row">
                    <div class="tg-introduction tg-introductionvtwo">
                        {% for i in 1..4 %}
                            {% if loop.index ==  3 %}
                                <!--************************************
                                        Home Banner Start
                                *************************************-->
                                <div id="tg-homebannervthree" class="tg-homebanner tg-homebannervthree">
                                    <figure class="item" data-vide-bg="poster: {{ asset('build/global/static/images/slider/img-13.jpg') }}" data-vide-options="position: 0% 50%">
                                        <figcaption>
                                            <div class="container">
                                                <div class="row">
                                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                                        <div class="tg-maingoal tg-bannercontent">
                                                            <div class="tg-btns">
                                                                <a class="tg-btnplay tg-btnpink" href="https://www.youtube.com/watch?v=2Fca03P-Vxg" data-rel="prettyPhoto[video]"><i class="icon-play3"></i></a>
                                                            </div>
                                                            <h4><span style="color:#ffffff;">{{'ce que nous faisons'|capitalize }}</span></h4>
                                                            <h1>La communication <span>au cœur du développement.</span></h1>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </figcaption>
                                    </figure>
                                </div>
                                <!--************************************
                                        Home Banner End
                                *************************************-->
                            {% endif %}
                            <div class="tg-mb10">
                                {% if loop.index is divisible by(2) %}
                                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                        <div class="row">
                                            <div class="tg-textshortcode">
                                                <h2><span>{{ 'Titre de la section ADN' }}</span></h2>
                                                <div class="tg-description">
                                                    <p>Lorem ipsum dolor sit amet, eu per legimus referrentur. Ius ne viris repudiare, nominavi sententiae eos in. Et duo salutatus consequat Lorem ipsum dolor sit amet.</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                        <div class="row">
                                            <div class="tg-video" data-vide-bg="poster: {{ asset('build/global/static/images/allhome/7/img-01.jpg') }}" data-vide-options="position: 0% 50%"></div>
                                        </div>
                                    </div>
                                {% else %}
                                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                        <div class="row">
                                            <div class="tg-video" data-vide-bg="poster: {{ asset('build/global/static/images/allhome/7/img-01.jpg') }}" data-vide-options="position: 0% 50%"></div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                        <div class="row">
                                            <div class="tg-textshortcode">
                                                <h2><span>{{ 'Titre de la section ADN' }}</span></h2>
                                                <div class="tg-description">
                                                    <p>Lorem ipsum dolor sit amet, eu per legimus referrentur. Ius ne viris repudiare, nominavi sententiae eos in. Et duo salutatus consequat Lorem ipsum dolor sit amet.</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {% endif %}
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </section>
        <!--************************************
                Content Area End
        *************************************-->
    <section class="tg-sectionspace tg-haslayout tg-paddingbottomzero">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-push-2 col-lg-8">
                    <div class="tg-sectionhead tg-sectionheadvtwo">
                        <div class="tg-sectiontitle">
                            <h2>{{'Ils nous font confiance'|upper }}</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--************************************
    Brands Start
    *************************************-->
    <div class="tg-sectionspace tg-haslayout tg-paddingbottomzero">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="tg-barandsslider" class="tg-barandsslider owl-carousel tg-barands">
                        {% for i in 1..6 %}
                            <div class="item"><figure><a href="javascript:void(0);(0);"><img src="{{ asset('build/global/static/images/allhome/2/img-10.jpg') }}" alt="image description"></a></figure></div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--************************************
            Brands End
    *************************************-->
{% endblock %}