{% extends 'base.html.twig' %}
{% block title %}{{ 'Le Cabinet | Impact-performances' }}{% endblock %}
{% block slide %}
    <!--************************************
            Home Slider Start
    *************************************-->
    <div id="tg-innerbanner" class="tg-innerbanner">
        <figure data-vide-bg="poster: {{ asset('build/global/images/innerpagebanner/img-05.jpg') }}" data-vide-options="position: 0% 50%">
            <figcaption>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <h1>{{ 'Le Cabinet' }}</h1>
                            <h2>Nous aidons nos clients à mesurer l’impact de leurs actions en les guidant <span> et en les accompagnant dans leurs choix stratégiques </span></h2>
                        </div>
                    </div>
                </div>
            </figcaption>
        </figure>
    </div>
    <!--************************************
            Home Slider End
    *************************************-->
{% endblock %}
