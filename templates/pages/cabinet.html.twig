    <!--************************************
        Creative Works Start
    *************************************-->
    <section class="tg-haslayout" data-z-index="1" data-appear-top-offset="600" data-parallax="scroll" data-image-src="{{ asset('build/global/static/images/parallax/bgparallax-10.jpg') }}">
        <div class="tg-sectionspace tg-parallax tg-parallaxcreativeworks tg-whitecontent">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-push-2 col-lg-8">
                        <div class="tg-sectionhead tg-sectionheadvtwo">
                            <div class="tg-sectiontitle">
                                <h2>{{ 'Notre approche' }}</h2>
                            </div>
                            <div class="tg-description">
                                <p>Mauris in rutrum nunc. Aenean in lectus id mi sollicitudin placerat Sed porta All Members</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="tg-themetabs tg-creativetabs">
                            <ul class="tg-themetabnav" role="tablist">
                                {% for i in 1..5 %}
                                    <li role="presentation" {% if loop.first %} class="active" {% endif %}><a href="#etape{{ loop.index }}" aria-controls="{{ loop.index }}" role="tab" data-toggle="tab">{{"Etape " ~ loop.index }}</a></li>
                                {% endfor %}
                            </ul>
                            <div class="tab-content tg-themetabcontent">
                                {% for i in 1..5 %}
                                    <div role="tabpanel" class="tab-pane {% if loop.first %} active {% endif %} fade in" id="etape{{ loop.index }}">
                                        <div class="row">
                                            <div class="tg-creativetabcontent">
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <h2>Approche {{ loop.index }}</h2>
                                                    <div class="tg-description">
                                                        <p>Lorem ipsum dolor sit amet, eu per legimus referrentur. Ius ne viris repudiare, nominavi sententiae eos in. Et duo salutatus consequat, Lorem ipsum dolor sit amet, eu per legimus referrentur.</p>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <figure><img src="{{ asset('build/global/static/images/img-01.jpg') }}" alt="{{ 'Approche' }}"></figure>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {% endfor %}
                            </div>
                        </div>
                    </div>




                </div>
            </div>
        </div>
    </section>
    <!--************************************
            Creative Works End
    *************************************-->

    <!--************************************
    Brands Start
    *************************************-->
    <div class="tg-sectionspace tg-haslayout tg-paddingbottomzero">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="tg-barandsslider" class="tg-barandsslider owl-carousel tg-barands">
                        {% for i in 1..6 %}
                            <div class="item"><figure><a href="javascript:void(0);(0);"><img src="{{ asset('build/global/static/images/allhome/2/img-10.jpg') }}" alt="image description"></a></figure></div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--************************************
